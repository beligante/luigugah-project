<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<%@ include file="../Taglibs.jsp" %>

<s:form action="updateprodutosubmit" cssStyle="color:black">

	<BR><BR>
	<s:hidden name="produto.id"></s:hidden>
	
	<s:textfield 
		label="CEP"
		name="produto.cep" 
		cssClass="form-control"
		cssStyle="background-image: url(data:image/png;base64,); padding-right: 0px; background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat no-repeat;"></s:textfield>	
	<s:textfield 
		label="Endereço"
		name="produto.endereco" 
		cssClass="form-control"
		cssStyle="background-image: url(data:image/png;base64,); padding-right: 0px; background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat no-repeat;"></s:textfield>	
	<s:textfield 
		label="Bairro"
		name="produto.bairro" 
		cssClass="form-control"
		cssStyle="background-image: url(data:image/png;base64,); padding-right: 0px; background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat no-repeat;"></s:textfield>	
	<s:textfield 
		label="Complemento"
		name="produto.complemento" 
		cssClass="form-control"
		cssStyle="background-image: url(data:image/png;base64,); padding-right: 0px; background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat no-repeat;"></s:textfield>	
	
	<s:select label="Estado" name="produto.estado" list="estados"></s:select>
	<s:select label="Cidade" name="produto.cidade" list="cidades"></s:select>
	<s:textfield 
		label="Dimensao"
		name="produto.dimensoes" 
		cssClass="form-control"
		cssStyle="background-image: url(data:image/png;base64,); padding-right: 0px; background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat no-repeat;"></s:textfield>	
		
	<s:select label="Tipo" name="produto.tipoProduto" list="tiposProduto"></s:select>
	<s:textarea label="Descrição" cssClass="form-control" name="produto.descricao"></s:textarea>

	<s:textfield 
		label="Preço"
		name="produto.preco" 
		cssClass="form-control"
		cssStyle="background-image: url(data:image/png;base64,); padding-right: 0px; background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat no-repeat;"></s:textfield>	

	<s:textfield 
		label="Link imagem"
		name="produto.linkFoto" 
		cssClass="form-control"
		cssStyle="background-image: url(data:image/png;base64,); padding-right: 0px; background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat no-repeat;"></s:textfield>	

	<s:submit label="Cadastrar"></s:submit>

</s:form>