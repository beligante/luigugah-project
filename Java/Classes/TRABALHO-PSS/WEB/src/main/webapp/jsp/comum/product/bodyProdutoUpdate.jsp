<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<%@ include file="../Taglibs.jsp" %>

<s:form action="updateprodutosubmit" cssStyle="color:black">

	<BR><BR>
	<s:hidden
		name="produto.id"
		value="<c:out value="${produto.id}"></c:out>"></s:hidden>
	<s:textfield 
		label="CEP"
		name="produto.cep" 
		value="<c:out value="${produto.cep}"></c:out>"
		cssClass="form-control"
		cssStyle="background-image: url(data:image/png;base64,); padding-right: 0px; background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat no-repeat;"></s:textfield>	
	<s:textfield 
		label="Endereço"
		name="produto.endereco" 
		value="<c:out value="${produto.endereco}"></c:out>"
		cssClass="form-control"
		cssStyle="background-image: url(data:image/png;base64,); padding-right: 0px; background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat no-repeat;"></s:textfield>	
	<s:textfield 
		label="Bairro"
		name="produto.bairro" 
		value="<c:out value="${produto.bairro}"></c:out>"
		cssClass="form-control"
		cssStyle="background-image: url(data:image/png;base64,); padding-right: 0px; background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat no-repeat;"></s:textfield>	
	<s:textfield 
		label="Complemento"
		value="<c:out value="${produto.complemento}"></c:out>"
		name="produto.complemento" 
		cssClass="form-control"
		cssStyle="background-image: url(data:image/png;base64,); padding-right: 0px; background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat no-repeat;"></s:textfield>	
	
	<s:select 
		label="Estado" 
		value="<c:out value="${produto.estado}"></c:out>"
		name="produto.estado" 
		list="estados">
		</s:select>
	
	<s:select 
		label="Cidade" 
		value="<c:out value="${produto.cidade}"></c:out>" 
		name="produto.cidade" 
		list="cidades"></s:select>
	
	<s:textfield 
		label="Dimensao"
		name="produto.dimensoes" 
		value="<c:out value="${produto.dimensoes}"></c:out>"
		cssClass="form-control"
		cssStyle="background-image: url(data:image/png;base64,); padding-right: 0px; background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat no-repeat;"></s:textfield>	
		
	<s:select 
		label="Tipo" 
		value="<c:out value="${produto.tipoProduto}"></c:out>"
		name="produto.tipoProduto" 
		list="tiposProduto">
		</s:select>
	
	<s:textarea 
		label="Descrição" 
		cssClass="form-control" 
		value="<c:out value="${produto.descricao}"></c:out>"
		name="produto.descricao">
		</s:textarea>

	<s:textfield 
		label="Preço"
		name="produto.preco" 
		value="<c:out value="${produto.preco}"></c:out>"
		cssClass="form-control"
		cssStyle="background-image: url(data:image/png;base64,); padding-right: 0px; background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat no-repeat;"></s:textfield>	

	<s:textfield 
		label="Link imagem"
		name="produto.linkFoto" 
		cssClass="form-control"
		value="<c:out value="${produto.linkFoto}"></c:out>"
		cssStyle="background-image: url(data:image/png;base64,); padding-right: 0px; background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat no-repeat;"></s:textfield>	

	<s:submit label="Cadastrar"></s:submit>

</s:form>